{
  "name": "emitnlog",
  "version": "0.13.0",
  "description": "Emit n' Log: a modern, type-safe library for logging, event notifications, and observability in JavaScript/TypeScript apps.",
  "author": {
    "name": "Marcelo Paternostro",
    "email": "mpaternostro@gmail.com"
  },
  "keywords": [
    "api",
    "notification",
    "logging",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/m-paternostro/emitnlog.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm-node/index.d.ts",
        "node": "./dist/esm-node/index.js",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs-node/index.d.cts",
        "node": "./dist/cjs-node/index.cjs",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./logger": {
      "import": {
        "types": "./dist/esm-node/logger/index.d.ts",
        "node": "./dist/esm-node/logger/index.js",
        "default": "./dist/esm/logger/index.js"
      },
      "require": {
        "types": "./dist/cjs-node/logger/index.d.cts",
        "node": "./dist/cjs-node/logger/index.cjs",
        "default": "./dist/cjs/logger/index.cjs"
      }
    },
    "./notifier": {
      "import": {
        "types": "./dist/esm-node/notifier/index.d.ts",
        "node": "./dist/esm-node/notifier/index.js",
        "default": "./dist/esm/notifier/index.js"
      },
      "require": {
        "types": "./dist/cjs-node/notifier/index.d.cts",
        "node": "./dist/cjs-node/notifier/index.cjs",
        "default": "./dist/cjs/notifier/index.cjs"
      }
    },
    "./tracker": {
      "import": {
        "types": "./dist/esm-node/tracker/index.d.ts",
        "node": "./dist/esm-node/tracker/index.js",
        "default": "./dist/esm/tracker/index.js"
      },
      "require": {
        "types": "./dist/cjs-node/tracker/index.d.cts",
        "node": "./dist/cjs-node/tracker/index.cjs",
        "default": "./dist/cjs/tracker/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/esm-node/utils/index.d.ts",
        "node": "./dist/esm-node/utils/index.js",
        "default": "./dist/esm/utils/index.js"
      },
      "require": {
        "types": "./dist/cjs-node/utils/index.d.cts",
        "node": "./dist/cjs-node/utils/index.cjs",
        "default": "./dist/cjs/utils/index.cjs"
      }
    },
    "./neutral": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./neutral/logger": {
      "import": {
        "types": "./dist/esm/logger/index.d.ts",
        "default": "./dist/esm/logger/index.js"
      },
      "require": {
        "types": "./dist/cjs/logger/index.d.cts",
        "default": "./dist/cjs/logger/index.cjs"
      }
    },
    "./neutral/notifier": {
      "import": {
        "types": "./dist/esm/notifier/index.d.ts",
        "default": "./dist/esm/notifier/index.js"
      },
      "require": {
        "types": "./dist/cjs/notifier/index.d.cts",
        "default": "./dist/cjs/notifier/index.cjs"
      }
    },
    "./neutral/tracker": {
      "import": {
        "types": "./dist/esm/tracker/index.d.ts",
        "default": "./dist/esm/tracker/index.js"
      },
      "require": {
        "types": "./dist/cjs/tracker/index.d.cts",
        "default": "./dist/cjs/tracker/index.cjs"
      }
    },
    "./neutral/utils": {
      "import": {
        "types": "./dist/esm/utils/index.d.ts",
        "default": "./dist/esm/utils/index.js"
      },
      "require": {
        "types": "./dist/cjs/utils/index.d.cts",
        "default": "./dist/cjs/utils/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "CHANGELOG.md",
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint",
    "lint": "eslint --fix",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "open ./coverage/index.html",
    "test:smoke": "node tests-smoke/run.js",
    "test:smoke:build": "node tests-smoke/run.js --build",
    "validate": "npm run typecheck && npm run build && npm run format:check && npm run lint:check",
    "postvalidate": "npm run test:coverage && npm run test:smoke",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-require-node-import-prefix": "^0.0.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "open": "^11.0.0",
    "prettier": "^3.7.4",
    "prettier-plugin-jsdoc": "^1.7.0",
    "@vitest/coverage-v8": "^4.0.15",
    "tsup": "^8.5.1",
    "type-fest": "^5.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}
