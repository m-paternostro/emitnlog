{
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "cSpell.words": [
    "Ansi",
    "Cayde",
    "closables",
    "cooldown",
    "cooldowns",
    "emitnlog",
    "errorify",
    "Finalizable",
    "flushables",
    "github",
    "GITHUB",
    "ndjson",
    "nullable",
    "Paternostro",
    "stringifiable",
    "stringifier",
    "Stringifier"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": "explicit", "source.removeUnusedImports": "explicit" },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.guides.bracketPairs": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.largeFileOptimizations": false,
  "editor.minimap.enabled": false,
  "editor.parameterHints.cycle": true,
  "editor.rulers": [120],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.probe": ["javascript", "typescript", "typescriptreact", "html", "markdown"],
  "eslint.useFlatConfig": true,
  "eslint.validate": ["javascript", "typescript", "typescriptreact", "html", "markdown"],
  "explorer.confirmDelete": false,
  "files.associations": { ".env": "properties" },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsc.autoDetect": "off",
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.colorCustomizations": { "titleBar.activeBackground": "#415539" },
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.revealIfOpen": true
}
